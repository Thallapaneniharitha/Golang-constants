1.program:
package main
import "fmt"
const KI = 6.78
func main() {
    const VIK = "haritha"
    fmt.Println("Hello", VIK)
    fmt.Println("Happy", KI, "Day")
    const Correct= false
    fmt.Println("Go rules?", Correct)
}
output:
Hello haritha
Happy 6.78 Day
Go rules? false

2.program:
package main
import "fmt"
func main() {
    const A= "JRV"
    var B = "Haritha"
    var helloWorld = A+ " " + B
    helloWorld += "!"
    fmt.Println(helloWorld) 
    fmt.Println(A == "JRV")   
    fmt.Println(B < A) 
}
output:
JRV Haritha!
true
true

3.program:
package main
import "fmt"
const Ki = 4.23
func main() {
 const trueConst = true
    type myBool bool
    var defaultBool = trueConst 
    var customBool myBool = trueConst 
    fmt.Println(defaultBool)
    fmt.Println(customBool)
}
output:
true
true

4.program
package main
import "fmt"
func main() {
   const LENGTH int = 30
   const WIDTH int = 9   
   var area int
   area = LENGTH * WIDTH
   fmt.Printf("value of area : %d", area)   
}
output:
value of area : 270

5.program:
package main
import (  
    "fmt"
)
func main() {  
    const (
        name = "haritha"
        age = 25
        country = "Rajampet"
    )
    fmt.Println(name)
    fmt.Println(age)
    fmt.Println(country)
output:
haritha
25
rajampet

