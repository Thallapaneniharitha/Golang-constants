7.program:
package main
import "fmt"
const PRODUCT string = "Rajampet"
const PRICE = 1000
func main() {
	fmt.Println(PRODUCT)
	fmt.Println(PRICE)
}
output:
Rajampet
1000

8.program:
package main
import "fmt"
const (
	
	QUANTITY = 60
	PRODUCT  = "shop"
	PRICE    = 70.12
	STOCK  = false
)

func main() {
	fmt.Println(QUANTITY)
	fmt.Println(PRICE)
	fmt.Println(PRODUCT)
	fmt.Println(STOCK)
}
output:
60
70.12
shop
false

9.program:
package main
import "fmt"
func main() {
    const name, country = "nellore", "India"
    const (
        stockName  string  = "TCS"
        stockPrice float64 = 108.90
    )
    fmt.Println(stockName, stockPrice)
        fmt.Println(name, country)
}
output:
TCS 108.9
nellore India

10.program:
package main
import "fmt"
const (
    year     = 365
    leapYear = int32(366)
)

func main() {
    hours := 24
    minutes := int32(60)
    fmt.Println(hours * year)
    fmt.Println(minutes * year)
    fmt.Println(minutes * leapYear)
}
output:
8760
21900
21960



